{
  "name": "mini-patchwork",
  "version": "1.0.0",
  "description": "A malleable software environment for collaborative work",
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "preview": "pnpm --filter gaios preview",
    "build": "pnpm -r build",
    "tsc": "pnpm -r exec tsc",
    "preinstall": "npx only-allow pnpm",
    "test": "pnpm --filter \"./os\" test",
    "make-tool": "node scripts/make-tool.mjs",
    "clean": "rm -rf **/node_modules **/dist",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md,html,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md,html,yaml,yml}\""
  },
  "pnpm": {
    "overrides": {
      "@automerge/automerge": "3.0.0"
    }
  },
  "keywords": [],
  "author": "Ink & Switch",
  "devDependencies": {
    "@types/react-dom": "^18.3.7",
    "@vitest/ui": "^2.1.9",
    "prettier": "^3.5.3",
    "prompts": "^2.4.2",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.9",
    "yargs-parser": "^21.1.1"
  }
}
